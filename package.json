{
  "name": "fable-jest",
  "version": "0.0.1",
  "description": "Fable bindings for jest and friends",
  "homepage": "https://github.com/Shmew/Fable.Jest",
  "bugs": {
    "url": "https://github.com/Shmew/Fable.Jest/issues/new/choose"
  },
  "license": "MIT",
  "author": "Cody Johnson",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shmew/Fable.Jest"
  },
  "scripts": {
    "build": "webpack -p",
    "compile-demo-raw": "fable-splitter -c docs/splitter.config.js",
    "pretestJest": "fable-splitter -c tests/Fable.Jest.Tests/splitter.config.js",
    "pretestRTL": "fable-splitter -c tests/Fable.Jest.ReactTestingLibrary.Tests/splitter.config.js",
    "publish-docs": "yarn build && node publish.js",
    "start": "webpack-dev-server",
    "test": "yarn pretestJest && yarn pretestRTL && yarn jest",
    "testJest": "yarn pretestJest && yarn jest",
    "testRTL": "yarn pretestRTL && yarn jest"
  },
  "dependencies": {
    "react": "^16.10",
    "react-dom": "^16.10",
    "react-highlight": "^0.12",
    "react-markdown": "^4.2",
    "source-map-support": "^0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.6",
    "@babel/plugin-transform-modules-commonjs": "^7.9",
    "@babel/preset-env": "^7.6",
    "@testing-library/jest-dom": "^5.2",
    "@testing-library/react": "^10.0",
    "@testing-library/user-event": "^10.0",
    "babel-loader": "^8.0",
    "core-js": "^3.2",
    "fable-compiler": "^2.4",
    "fable-loader": "^2.1",
    "file-loader": "^4.2",
    "gh-pages": "^2.1",
    "html-webpack-plugin": "^3.2",
    "jest": "^25.2",
    "mkdirp": "^0.5",
    "remotedev": "^0.2",
    "resolve-url-loader": "^3.1",
    "webpack": "^4.41",
    "webpack-cli": "^3.3",
    "webpack-dev-server": "^3.8"
  },
  "private": true
}